#include <stdio.h>
#include <stdlib.h>


int print_array(int *array){
	if (array == NULL)
		return -1;
	int size = sizeof(array) / sizeof(array[0]);
	printf("[ %", size);
	for (int i = 0; i < size; i++)
		printf(" %d ",array[i]);
	printf("]\n");
		       return 0;
}
/**
   int arr[] = { 12, 11, 13, 5, 6 };
   int arr[] = { 11, 11, 13, 5, 6 };
   int arr[] = { 11, 12, 13, 5, 6 };
   int arr[] = { 12, 11, 13, 5, 6 };

 */
int insetion_sort(int *array)
{
	if (array == NULL)
		return -1;

	int key = array[0];

	int size = sizeof(array) / sizeof(array[0]);
	for (int i = 0; i < size; i++)
	{
		printf("sorting \n");
		print_array(array);
		int j = 1 + i;
		if (array[i] > array[j])
			while (j >= 0){
				array[i] = array[j];
				j--;
			}
		array[i] = key;
	}
	printf("sorting \n");

}

int main(){
	int arr[] = { 12, 11, 13, 5, 6 };
	print_array(arr);
	insetion_sort(arr);
	print_array(arr);
	return 0;
}
